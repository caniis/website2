    <h3>顏色選擇</h3>
        <form>
            <input type="color" id="f3"><p></p>
        </form>

    <h3>檔案選擇</h3>
        <form>
            <input type="file" id="f7"><p></p>
        </form>

    <h3>數字選擇</h3>
        <form>
            <input type="number" id="f11" value="10" min="10" max="20"><p></p>
        </form>

    <h3>range</h3>
            <input type="range" id="f13" min="10" max="20"><p></p>
        </form>

    <h3>選擇</h3>
        <form>
            <input type="radio" value="f" name="gender">女 
            <input type="radio" value="m" name="gender">男
        </form>

    <h3>文字框</h3>
        <form>
            <textarea cols="40" rows="5"></textarea>
        </form>

    <h3>選擇</h3>
        <form>
            <label for="e1">教育程度</label>
            <select id="e1">
              <option value="1">高中(含)以下</option>
              <option value="2">大專</option>
              <option value="3">研究所以上1</option>
            </select>
